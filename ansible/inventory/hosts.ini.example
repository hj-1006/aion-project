# AION Ansible inventory - copy to hosts.ini and set your devices
[routers]
Router2 ansible_host=192.168.1.2 ansible_user=admin ansible_password=secret ansible_connection=network_cli ansible_network_os=ios
Router3 ansible_host=192.168.1.3 ansible_user=admin ansible_password=secret ansible_connection=network_cli ansible_network_os=ios

[routers:vars]
ansible_become=yes
ansible_become_method=enable
ansible_become_password=enable_secret
